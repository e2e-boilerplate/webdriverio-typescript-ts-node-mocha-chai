{
  "author": "Girma Nigusse <xgirma@gmail.com> (https://github.com/xgirma)",
  "description": "WebdriverIO (sync) end-to-end test automation boilerplate, using TypeScript, ES6 modules, ts-node, Mocha, Chai and Expect",
  "devDependencies": {
    "@types/chai": "4.2.10",
    "@types/mocha": "7.0.1",
    "@types/node": "12.12.29",
    "@wdio/cli": "5.18.7",
    "@wdio/dot-reporter": "5.18.6",
    "@wdio/local-runner": "5.18.7",
    "@wdio/mocha-framework": "5.18.7",
    "@wdio/sync": "5.18.7",
    "chai": "4.2.0",
    "chromedriver": "79.0.3",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "mocha": "7.1.0",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "ts-node": "8.6.2",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.8.3",
    "wdio-chromedriver-service": "5.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "boilerplate",
    "chai",
    "e2e",
    "e2e tests",
    "example",
    "integration test",
    "mocha",
    "ts-node",
    "typescript",
    "webdriverio"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{ts,js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "name": "webdriverio-typescript-ts-node-mocha-chai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/e2e-boilerplates/webdriverio-typescript-ts-node-mocha-chai.git"
  },
  "scripts": {
    "lint": "npx tslint -c tslint.json './spec/*.ts'",
    "pretty": "npx prettier --no-config './spec/*.ts' --write",
    "test": "npx wdio"
  },
  "version": "1.0.0"
}
